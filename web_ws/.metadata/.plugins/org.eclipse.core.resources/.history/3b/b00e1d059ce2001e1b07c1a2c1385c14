<script>
	let snack = {
			name:"맛동산",
			price:4800,
			
			hi: function(){
				alert("hi);
				
			}
	,
			getPrice: function(){
				return this.price;
			}
	}
	
	snack.hi();
	let price = snack.getPrice();
	alert(price);
</script>
