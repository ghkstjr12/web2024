package day6;

public class Book {
   String title;
   String author;
   int price;
   
   public Book() {
      // TODO Auto-generated constructor stub
   }

   public String getTitle() {
      return title;
   }

   public void setTitle(String title) {
      this.title = title;
   }

   public String getAuthor() {
      return author;
   }

   public void setAuthor(String author) {
      this.author = author;
   }

   public int getPrice() {
      return price;
   }

   public void setPrice(int price) {
      this.price = price;
   }

   public Book(String title, String author, int price) {
      
      this.title = title;
      this.author = author;
      this.price = price;
   }
   
   
   
}
